{
    "_version": "1.8.0",
    "sap.app": {
        "id": "com.sap.espm.shop",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.40.12"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "com.sap.espm.shop.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.ui.layout": {},
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.sap.espm.shop.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.sap.espm.shop.view",
                "controlAggregation": "pages",
                "controlId": "idAppControl",
                "clearControlAggregation": false,
                "bypassed": {
                    "target": "notfound"
                }
            },
            "routes": [
                
                {
                    "pattern": "",
                    "name": "landing",
                    "target": "landing"
                },
                {
                    "pattern": "Register",
                    "name": "Register",
                    "target": "Register"
                },
                {
                    "pattern": "customer",
                    "name": "customer",
                    "target": "customer"
                },
                {
                    "pattern": "Product",
                    "name": "Product",
                    "target": "Product"
                },
                {
                    "pattern": "SalesOrder/{SalesOrder}",
                    "name": "SalesOrder",
                    "target": "SalesOrder"
                },
                {
                    "pattern": "ProductDetail/{Productdetails}",
                    "name": "ProductDetail",
                    "target": "ProductDetail"
                }, {
                    "pattern": "Shoppingcart",
                    "name": "Shoppingcart",
                    "target": "Shoppingcart"
                }, {
                    "pattern": "Checkout",
                    "name": "Checkout",
                    "target": "Checkout"
                }
            ],
            "targets": {
                "app": {
                    "viewType": "XML",
                    "viewName": "App",
                    "routerClass": "sap.m.routing.Router",
                    "async": true,
                    "viewPath": "com.sap.espm.shop.view",
                    "controlAggregation": "pages",
                    "controlId": "idAppControl",
                    "clearControlAggregation": false,
                    "viewLevel": 1,
                    "bypassed": {
                        "target": "notfound"
                    }
                },
                "customer": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "Customer",
                    "routerClass": "sap.m.routing.Router",
                    "async": true,
                    "viewPath": "com.sap.espm.shop.view",
                    "controlAggregation": "pages",
                    "controlId": "idAppControl",
                    "viewLevel": 2,
                    "bypassed": {
                        "target": "notfound"
                    }
                },
                "landing": {
                    "viewType": "XML",
                    "viewName": "Landing",
                    "routerClass": "sap.m.routing.Router",
                    "async": true,
                    "viewPath": "com.sap.espm.shop.view",
                    "controlAggregation": "pages",
                    "controlId": "idAppControl",
                    "clearControlAggregation": false,
                    "viewLevel": 2,
                    "bypassed": {
                        "target": "notfound"
                    }
                },
                "Register": {
                    "viewName": "Register"
                },
                "Product": {
                    "viewType": "XML",
                    "viewName": "Product",
                    "routerClass": "sap.m.routing.Router",
                    "async": true,
                    "viewPath": "com.sap.espm.shop.view",
                    "controlAggregation": "pages",
                    "controlId": "idAppControl",
                    "clearControlAggregation": false,
                    "viewLevel": 2,
                    "bypassed": {
                        "target": "notfound"
                    }
                },
                "ProductDetail": {
					"viewName": "ProductDetail"
				},
				"Shoppingcart": {
					"viewName": "Shoppingcart"
				},
				"Checkout": {
					"viewName": "Checkout"
				},
                "SalesOrder": {
                    "viewType": "XML",
                    "viewName": "SalesOrder",
                    "routerClass": "sap.m.routing.Router",
                    "async": true,
                    "viewPath": "com.sap.espm.shop.view",
                    "controlAggregation": "pages",
                    "controlId": "idAppControl",
                    "clearControlAggregation": false,
                    "viewLevel": 3,
                    "bypassed": {
                        "target": "notfound"
                    }
                },
                "notfound": {
                    "viewType": "XML",
                    "viewName": "NotFound",
                    "routerClass": "sap.m.routing.Router",
                    "async": true,
                    "viewPath": "com.sap.espm.shop.view",
                    "controlAggregation": "pages",
                    "controlId": "idAppControl",
                    "clearControlAggregation": false,
                    "bypassed": {
                        "target": "notfound"
                    }
                },
                "AddItemToCart": {
                    "viewType": "XML",
                    "viewName": "AddItemToCart",
                    "routerClass": "sap.m.routing.Router",
                    "async": true,
                    "viewPath": "com.sap.espm.shop.view",
                    "controlAggregation": "pages",
                    "controlId": "idAppControl",
                    "clearControlAggregation": false,
                    "bypassed": {
                        "target": "notfound"
                    }
                },
                "SubmitOrder": {
                    "viewType": "XML",
                    "viewName": "SubmitOrder"
                }
            }
        }
    }
}